generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Comment {
  id           String   @id @default(cuid())
  name         String
  relationship String?  @db.VarChar(64)
  message      String   @db.VarChar(500)
  createdAt    DateTime @default(now())
}